@font-face {
  font-family: "Flaticon";
  src: url('<%= asset_path("fonts/Flaticon.eot") %>');
  src: url('<%= asset_path("fonts/Flaticon.eot?#iefix") %>') format("embedded-opentype"),
       url('<%= asset_path("fonts/Flaticon.woff") %>') format("woff"),
       url('<% asset_path("fonts/Flaticon.ttf") %>') format("truetype"),
       url('<%= asset_path("fonts/Flaticon.svg#Flaticon") %>') format("svg");
  font-weight: normal;
  font-style: normal;
}



@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: "Flaticon";
    src: url('<%= asset_path("fonts/Flaticon.svg#Flaticon") %>') format("svg");
  }
}
